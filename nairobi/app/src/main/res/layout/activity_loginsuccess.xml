<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".loginsuccess">


    <TextView
        android:id="@+id/tvtext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.245" />

    <Button
        android:id="@+id/ambtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="getData2"
        android:text="show meter data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvtext"
        app:layout_constraintVertical_bias="0.206" />

    <Button
        android:id="@+id/gbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="224dp"
        android:onClick="getData3"
        android:text="show meter location"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ambtn"
        app:layout_constraintVertical_bias="0.623" />

    <Button
        android:id="@+id/ambt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="getData4"
        android:text="show all meters data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gbtn"
        app:layout_constraintVertical_bias="0.29" />

    <Button
        android:id="@+id/vbtn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Visualize data"
        android:onClick="getData5"
        app:layout_constraintBottom_toTopOf="@+id/ambtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvtext"
        app:layout_constraintVertical_bias="0.38" />

    <Button
        android:id="@+id/vbtn2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="visualize channel"
        android:onClick="getData6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ambt" />

</androidx.constraintlayout.widget.ConstraintLayout>